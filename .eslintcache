ns/crescendo-frontend/src/Components/Welcome.js",["51","52","53","54"],"import React, { useState, useEffect } from \"react\";\nimport { useHistory } from \"react-router-dom\";\nimport { Container, Spinner, Row, Col } from \"react-bootstrap\";\nimport { Card, Button } from \"@material-ui/core\";\n\nimport CloudUploadIcon from \"@material-ui/icons/CloudUpload\";\n\nimport { uploadFile, getFiles } from \"../Apis/FileUpload\";\n\nconst Welcome = () => {\n  const [selectedFiles, setSelectedFiles] = useState(undefined);\n  const [currentFile, setCurrentFile] = useState(undefined);\n  const [progress, setProgress] = useState(0);\n  const [message, setMessage] = useState(\"\");\n  const history = useHistory();\n\n  const selectFile = (event) => {\n    setSelectedFiles(event.target.files);\n  };\n\n  const upload = () => {\n    let currentFile = selectedFiles[0];\n    setProgress(0);\n    setCurrentFile(currentFile);\n\n    uploadFile(currentFile, (event) => {\n      setProgress(Math.round((100 * event.loaded) / event.total));\n    })\n      .then((response) => {\n        setMessage(response.data.message);\n        localStorage.setItem(\"client_id\", response.data.client_id);\n        localStorage.setItem(\"output_folder\", response.data.output_folder);\n        history.push(\"stems\");\n      })\n      .catch(() => {\n        setProgress(0);\n        setMessage(\"Could not upload the file!\");\n        setCurrentFile(undefined);\n      });\n\n    setSelectedFiles(undefined);\n  };\n  return (\n    <>\n      <Container fluid className=\" mx-0 w-100\">\n        <h1 className=\"mt-5\">Welcome to Crescendo!</h1>\n        <Card className=\"upload-card mt-5 px-5 mx-auto\">\n          <h3 className=\"mt-5\">Upload an audio file to proceed</h3>\n\n          {currentFile && (\n            <div className=\"progress mt-6\">\n              <div\n                className=\"progress-bar progress-bar-info progress-bar-striped\"\n                role=\"progressbar\"\n                aria-valuenow={progress}\n                aria-valuemin=\"0\"\n                aria-valuemax=\"100\"\n                style={{ width: progress + \"%\" }}\n              >\n                {progress}%\n              </div>\n            </div>\n          )}\n\n          <label className=\"btn btn-default\">\n            <input type=\"file\" onChange={selectFile} />\n          </label>\n\n          <Button\n            variant=\"contained\"\n            color=\"default\"\n            disabled={!selectedFiles}\n            onClick={upload}\n            className=\"w-7rem\"\n            startIcon={<CloudUploadIcon />}\n          >\n            Upload\n          </Button>\n          <div className=\"alert alert-light\" role=\"alert\">\n            {message}\n          </div>\n\n          {progress === 100 ? (\n            <Button variant=\"primary\" disabled>\n              <Spinner\n                as=\"span\"\n                animation=\"grow\"\n                size=\"sm\"\n                role=\"status\"\n                aria-hidden=\"true\"\n              />\n              Processing File ...\n            </Button>\n          ) : (\n            <> </>\n          )}\n        </Card>\n      </Container>\n    </>\n  );\n};\n\nexport default Welcome;\n","/Users/capriciousrebel/Desktop/Hackathons/crescendo-frontend/src/Components/Templates.js",["55"],"/Users/capriciousrebel/Desktop/Hackathons/crescendo-frontend/src/Components/Animation.js",["56"],"import React from \"react\";\nimport { Container } from \"react-bootstrap\";\n\n/**\n * @name PropertyListView\n * @description Renders the Property List View\n */\n\nconst Animation = (props) => {\n  return (\n    <>\n      <h1>Map objects to animation!</h1>\n    </>\n  );\n};\n\nexport default Animation;\n","/Users/capriciousrebel/Desktop/Hackathons/crescendo-frontend/src/Apis/FileUpload.js",[],"/Users/capriciousrebel/Desktop/Hackathons/crescendo-frontend/src/Apis/AxiosInstance.js",[],"/Users/capriciousrebel/Desktop/Hackathons/crescendo-frontend/src/Components/Stems.js",["57","58","59","60","61","62"],"import React, { useState, useEffect } from \"react\";\nimport { useHistory, Link } from \"react-router-dom\";\nimport { Container, Spinner, Row, Col } from \"react-bootstrap\";\nimport { Card, Button } from \"@material-ui/core\";\nimport { uploadFile, getFiles } from \"../Apis/FileUpload\";\nimport ReactPlayer from \"react-player\";\nimport NavigateNextIcon from \"@material-ui/icons/NavigateNext\";\n\nconst Stems = () => {\n  const [stems, setStems] = useState([]);\n  const startStream = () => {\n    getFiles(\n      localStorage.getItem(\"client_id\"),\n      localStorage.getItem(\"output_folder\")\n    ).then((response) => {\n      setStems(response.data);\n    });\n  };\n  useEffect(() => {\n    startStream();\n  }, []);\n  return (\n    <>\n      <Col>\n        <Row>\n          <Col xl={2}></Col>\n          <Col xl={8} className=\"mt-5\">\n            <h1>Here are your seperated Stems!</h1>\n          </Col>\n          <Col xl={2} className=\"mt-5  pt-2\">\n            <Link to=\"/templates\" className=\"decoration-none\">\n              <h4>Choose a template</h4>\n            </Link>\n          </Col>\n        </Row>\n        <Row>\n          {stems.map((stem) => (\n            <Card className=\"stem-card mt-5 mx-auto d-flex flex-column align-items-center justify-content-around\">\n              <h5>{stem.name}</h5>\n              <ReactPlayer\n                url={stem.url}\n                width=\"20rem\"\n                height=\"3rem\"\n                playing={false}\n                controls={true}\n              />\n            </Card>\n          ))}\n        </Row>\n      </Col>\n    </>\n  );\n};\n\nexport default Stems;\n","/Users/capriciousrebel/Desktop/Hackathons/crescendo-frontend/src/Components/Dropdown.js",[],"/Users/capriciousrebel/Desktop/Hackathons/crescendo-frontend/src/Apis/Templates.js",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67